IMPORT              KW_IMPORT       
Write               IDENT           Write
,                   ,               
WriteCard           IDENT           WriteCard
,                   ,               
WriteString         IDENT           WriteString
,                   ,               
WriteLn             IDENT           WriteLn
;                   ;               
CONST               KW_CONST        
FromAscii           IDENT           FromAscii
=                   =               
32                  INTEGER_CONST   32
;                   ;               
ToAscii             IDENT           ToAscii
=                   =               
127                 INTEGER_CONST   127
;                   ;               
