)                   )               
;                   ;               
Write               IDENT           Write
(                   (               
CHR                 IDENT           CHR
(                   (               
i                   IDENT           i
)                   )               
)                   )               
;                   ;               
WriteLn             IDENT           WriteLn
END                 KW_END          
;                   ;               
fl                  IDENT           fl
:=                  ASSIGN          
1.1                 REAL_CONST      1.1
+                   +               
1.0E-2              REAL_CONST      1.0E-2
+                   +               
1.0E+2              REAL_CONST      1.0E+2
+                   +               
1.0E1               REAL_CONST      1.0E1
;                   ;               
IF                  KW_IF           
(                   (               
fl                  IDENT           fl
<=                  LE              
11.11               REAL_CONST      11.11
)                   )               
AND                 KW_AND          
(                   (               
fl                  IDENT           fl
>=                  GE              
1.111E1             REAL_CONST      1.111E1
)                   )               
THEN                KW_THEN         
WriteString         IDENT           WriteString
(                   (               
"Zgodnie z oczekiwanSTRING_CONST    "Zgodnie z oczekiwaniami"
)                   )               
ELSE                KW_ELSE         
WriteString         IDENT           WriteString
(                   (               
"Olaboga!"          STRING_CONST    "Olaboga!"
)                   )               
END                 KW_END          
;                   ;               
WriteLn             IDENT           WriteLn
;                   ;               
i                   IDENT           i
:=                  ASSIGN          
1                   INTEGER_CONST   1
;                   ;               
WHILE               KW_WHILE        
i                   IDENT           i
<                   <               
5                   INTEGER_CONST   5
DO                  KW_DO           
WriteLn             IDENT           WriteLn
(                   (               
i                   IDENT           i
)                   )               
;                   ;               
i                   IDENT           i
:=                  ASSIGN          
i                   IDENT           i
+                   +               
1                   INTEGER_CONST   1
END                 KW_END          
;                   ;               
