WriteLn             IDENT           WriteLn
(                   (               
i                   IDENT           i
)                   )               
;                   ;               
i                   IDENT           i
:=                  ASSIGN          
i                   IDENT           i
-                   -               
1                   INTEGER_CONST   1
UNTIL               KW_UNTIL        
i                   IDENT           i
=                   =               
1                   INTEGER_CONST   1
;                   ;               
LOOP                KW_LOOP         
